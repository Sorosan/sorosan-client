    "lint": "tsc",
    "typedoc": "npx typedoc --options typedoc-options.json"
  },
  "devDependencies": {
    "docusaurus-plugin-typedoc": "^0.20.2",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typedoc": "^0.25.1",
    "typedoc-plugin-markdown": "^3.16.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.5"
  },
  "dependencies": {
    "@stellar/freighter-api": "^1.7.1",
    "@types/node": "^20.9.0",
    "soroban-client": "^1.0.0-beta.4"
  }
}
